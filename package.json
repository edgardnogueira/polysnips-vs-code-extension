{
  "name": "fullstack-snippets",
  "displayName": "FullStack vscode snippets",
  "description": "Snippets para desenvolvimento fullstack incluindo JavaScript, TypeScript, Go, PHP, Docker e mais",
  "version": "0.0.11",
  "publisher": "my-publisher-id",
  "repository": {
    "type": "git",
    "url": "https://github.com/edgardnogueira/vs-code-snippetss"
  },
  "keywords": [
    "snippets",
    "javascript",
    "typescript",
    "go",
    "php",
    "docker",
    "gorm",
    "jest",
    "sql"
  ],
  "author": {
    "name": "Seu Nome"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgardnogueira/vs-code-snippetss/issues"
  },
  "homepage": "https://github.com/edgardnogueira/vs-code-snippetss#readme",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/golang.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/golang-tests.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/go-wire.code-snippets"
      },
      {
        "language": "php",
        "path": "./snippets/php.code-snippets"
      },
      {
        "language": "docker",
        "path": "./snippets/docker.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/jest.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/jest.code-snippets"
      },
      {
        "language": "sql",
        "path": "./snippets/sql.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "package-extension": "vsce package",
    "install-extension": "code --install-extension"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
